									<xs:all> 
										<xs:element name="usage_type" type="xs:string"/>
										<xs:element name="project_id" type="xs:normalizedString"/>
										<xs:element name="machine_name" type="xs:normalizedString"/>
										<xs:element name="record_identity"> 

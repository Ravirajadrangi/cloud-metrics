							<xs:element name="header" type="header_type"/>
							<xs:element name="body">
								<xs:complexType>
									<xs:all>
										<xs:element name="project_id" type="xs:normalizedString"/>
										<xs:element name="dn_list" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="dn" type="xs:normalizedString" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element ref="reason" minOccurs="0"/>
										<xs:element name="comment" type="xs:string" minOccurs="0"/>
										<xs:element name="person_id" type="xs:normalizedString" minOccurs="0"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
